{
  "model_name": "Support Vector Machine",
  "model_key": "svm",
  "model_version": "v1",
  "problem_type": "binary_classification",
  "domain": "cardiovascular_risk_prediction",

  "training": {
    "algorithm": "SVC",
    "kernel": "rbf",
    "C": 1.0,
    "gamma": "scale",
    "probability": true,
    "random_state": 42
  },

  "data": {
    "num_features": 18,
    "feature_names": [
      "Chest_Pain",
      "Shortness_of_Breath",
      "Fatigue",
      "Palpitations",
      "Dizziness",
      "Swelling",
      "Pain_Arms_Jaw_Back",
      "Cold_Sweats_Nausea",
      "High_BP",
      "High_Cholesterol",
      "Diabetes",
      "Smoking",
      "Obesity",
      "Sedentary_Lifestyle",
      "Family_History",
      "Chronic_Stress",
      "Gender",
      "Age"
    ],
    "target": "Heart_Disease"
  },

  "artifacts": {
    "model_file": "svm_model.pkl",
    "scaler_file": "svm_scaler.pkl",
    "metrics_file": "svm_metrics.pkl"
  },

  "evaluation": {
    "metrics_included": [
      "accuracy",
      "precision",
      "recall",
      "f1_score",
      "roc_auc",
      "confusion_matrix",
      "training_time_minutes"
    ]
  },

  "environment": {
    "python_version": "3.10+",
    "framework": "scikit-learn",
    "library_versions": {
      "scikit_learn": ">=1.3",
      "numpy": ">=1.23",
      "pandas": ">=1.5"
    }
  },

  "status": {
    "stage": "production",
    "approved": true,
    "created_at": "2026-01-23",
    "last_updated": "2026-01-23"
  },

  "owner": {
    "team": "AI/ML Team",
    "project": "Multi-Factor Cardiovascular Risk Prediction"
  }
}
